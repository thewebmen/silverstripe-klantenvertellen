{
    "name": "thewebmen/silverstripe-klantenvertellen",
    "description": "Silverstripe module for the dutch review website klantenvertellen",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "reviews",
        "klantenvertellen",
        "dutch"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Michel van der Steege",
        "email": "michel@thewebmen.com"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "silverstripe/cms": "^4.0@dev",
        "silverstripe/vendor-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "installer-name": "silverstripe-klantenvertellen",
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "TheWebmen\\KlantenVertellen\\": "src/"
        }
    }
}
